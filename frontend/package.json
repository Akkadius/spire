{
  "name": "spire",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node --max_old_space_size=4096 --stack-size=10000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --open",
    "serve": "node --max_old_space_size=4096 --stack-size=10000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve",
    "build": "NODE_ENV=production node --max_old_space_size=4096 --stack-size=10000 node_modules/@vue/cli-service/bin/vue-cli-service.js build --nomaps",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
