    "ag-grid-community": "^25.3.0",
    "ag-grid-enterprise": "^25.3.0",
    "ag-grid-vue": "^25.3.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.21.2",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^2.6.12",
    "fontawesome-4.7": "^4.7.0",
    "jquery": "^3.6.0",
    "markdown-it": "^12.2.0",
    "moment": "^2.29.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "rpg-awesome": "^0.2.0",
    "sl-vue-tree": "^1.8.5",
    "slugify": "^1.6.3",
    "superagent": "^6.1.0",
